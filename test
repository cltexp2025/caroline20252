function onEachFeature(feature, layer) {
  const name = feature.properties.NAME;
  const links = countyLinks[name] || [];
  let popupContent = `<strong>${name} County</strong>`;
  if (links.length > 0) {
    popupContent += '<ul>' + links.map(l => `<li><a href="${l}" target="_blank">${l}</a></li>`).join('') + '</ul>';
  } else {
    popupContent += '<p>No links available.</p>';
  }
  layer.bindPopup(popupContent);
  layer.bindTooltip(name, {
    permanent: true,
    direction: 'center',
    className: 'county-label'
  });
}


L.geoJSON(geojson, {
  onEachFeature: onEachFeature,
  style: function(feature) {
    return {
      color: '#444',
      weight: 1,
      fillColor: countyColors[feature.properties.NAME] || '#ccc',
      fillOpacity: 0.7
    };
  }
}).addTo(map);
</script>


